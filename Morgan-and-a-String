#include <bits/stdc++.h>
using namespace std;

string morganAndString(string a, string b) {
    a.push_back('Z' + 1); // sentinel
    b.push_back('Z' + 1); // sentinel
    string res;
    int i = 0, j = 0;
    while (i < (int)a.size() - 1 || j < (int)b.size() - 1) {
        if (a.substr(i) <= b.substr(j))
            res.push_back(a[i++]);
        else
            res.push_back(b[j++]);
    }
    return res;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        string a, b;
        cin >> a >> b;
        cout << morganAndString(a, b) << "\n";
    }
    return 0;
}
